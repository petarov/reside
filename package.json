{
  "name": "reside",
  "version": "0.0.1",
  "description": "Reside is a minimalistic Java ResourceBundle files editor.",
  "main": "./src/main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "repository": "https://github.com/petarov/reside",
  "keywords": [
    "java",
    "ResourceBundle",
    "properties",
    "translations"
  ],
  "author": "Petar Petrov",
  "license": "MIT",
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "electron-forge": "^5.2.2",
    "electron-prebuilt-compile": "2.0.3"
  },
  "dependencies": {
    "electron-compile": "^6.4.3",
    "electron-reload": "^1.2.3",
    "electron-squirrel-startup": "^1.0.0",
    "framework7": "^3.0.0-beta.15",
    "framework7-icons": "^0.9.1",
    "install": "^0.12.1",
    "material-icons": "^0.2.3"
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "yarn"
      },
      "electronWinstallerConfig": {
        "name": "reside"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "petarov",
        "name": "reside"
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "reside"
      }
    }
  }
}

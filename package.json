{
  "name": "reside",
  "version": "0.0.9",
  "description": "Reside is a minimalistic Java ResourceBundle files editor.",
  "main": "./src/main.js",
  "scripts": {
    "start": "electron-forge start -- withdebug nosplash",
    "demo": "electron-forge start",
    "package": "electron-forge package",
    "dist": "electron-forge package --platform=win32,darwin",
    "make": "electron-forge make",
    "test": "mocha"
  },
  "repository": "https://github.com/petarov/reside",
  "keywords": [
    "java",
    "ResourceBundle",
    "properties",
    "translations"
  ],
  "author": "Petar Petrov",
  "license": "MIT",
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "electron-forge": "^5.2.2",
    "electron-prebuilt-compile": "2.0.3",
    "mocha": "^5.2.0"
  },
  "dependencies": {
    "electron-compile": "^6.4.3",
    "electron-reload": "^1.2.5",
    "electron-squirrel-startup": "^1.0.0",
    "framework7": "^3.0.0",
    "jsesc": "^2.5.1",
    "material-icons": "^0.2.3",
    "sanitize-filename": "^1.6.1",
    "template7": "^1.3.6"
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "yarn",
        "icon": "src/assets/icons/cat-vampire-icon"
      },
      "electronWinstallerConfig": {
        "name": "reside"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "petarov",
        "name": "reside"
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "reside"
      }
    }
  }
}
